{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "react-hooks", "unused-imports"],
  "rules": {
    "unused-imports/no-unused-imports-ts": "error",
    "@typescript-eslint/no-explicit-any": "warn"
  }
}